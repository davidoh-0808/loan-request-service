<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%>

	<!-- Slider main container -->
	<article class="swiper">
		<!-- Additional required wrapper -->
		<div class="swiper-wrapper">
			<!-- Slides -->
			<div class="swiper-slide">
				<div class="inner">
					<h2 class="title_slide">대출종류</h2>
					<p class="desc">
						현대차 미소 금융 재단 대출대상<br />적격여부를 알려드립니다.
					</p>
					<button type="button" class="btn btn_md btn_blue">자세히보기</button>
				</div>
			</div>
			<div class="swiper-slide">
				<div class="inner">
					<h2 class="title_slide">대출 상담신청</h2>
					<p class="desc">
						현대차 미소 금융 재단 상담 신청<br />안내해드립니다.
					</p>
					<button type="button" class="btn btn_md btn_blue">자세히보기</button>
				</div>
			</div>
			<div class="swiper-slide">
				<div class="inner">
					<h2 class="title_slide">대출 상품 안내</h2>
					<p class="desc">
						현대차 미소 금융 재단 대출가능한<br />상품을 안내해드립니다. 
					</p>
					<button type="button" class="btn btn_md btn_blue">자세히보기</button>
				</div>
			</div>
			<div class="swiper-slide">
				<div class="inner">
					<h2 class="title_slide">지점안내
					</h2>
					<p class="desc">
						현대차 미소 금융 재단 지점 위치를<br />안내해드립니다.
					</p>
					<button type="button" class="btn btn_md btn_blue">자세히보기</button>
				</div>
			</div>
		</div>
		<!-- If we need pagination -->
		<div class="swiper-pagination"></div>
	</article>